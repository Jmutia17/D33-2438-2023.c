// Using for loop
for (i = 1; i <= n; i++) {
    printf("Number is: %d and cube of the %d is: %d\n", i, i, i * i * i);
}

// Using while loop
while (i <= n) {
    printf("Number is: %d and cube of the %d is: %d\n", i, i, i * i * i);
    i++;
}

// Using do while loop
do {
    printf("Number is: %d and cube of the %d is: %d\n", i, i, i * i * i);
    i++;
} while (i <= n);
